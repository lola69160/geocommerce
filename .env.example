# Backend - Google APIs (REQUIRED)
# Gemini API Key - Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Places API Key - Required for PlacesAgent and location enrichment
PLACE_API_KEY=your_google_places_api_key_here

# Tavily API Key - Optional for enhanced demographic context
# Get from: https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# Server Configuration
PORT=3001

# ADK Configuration (OPTIONAL)
# Logging level for ADK operations
ADK_LOG_LEVEL=info

# Session TTL in seconds (default: 1 hour)
ADK_SESSION_TTL=3600
